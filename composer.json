{
  "name": "s008nyx/kafka-bus",
  "description": "Kafka bus integration to Laravel/Lumen",
  "keywords": [
    "laravel",
    "kafka",
    "bus"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexey Mitroshkin",
      "email": "sonyxvlg@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^8.0 || ^7.0",
    "illuminate/contracts": "^8.0 || ^7.0",
    "illuminate/config": "^8.0 || ^7.0",
    "ext-json": "*",
    "ext-rdkafka": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*",
    "mockery/mockery": "1.*"
  },
  "autoload": {
    "psr-4": {
      "KafkaBus\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KafkaBus\\KafkaBusServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
